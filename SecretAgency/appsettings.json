    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "Swagger": {
    "ApiName": "SecretAgency"
  },
  "Authentication": {
    "Auth0": {
      "Domain": "",
      "Audience": "",
      "ClientId": ""
    }
  },
  "Database": {
    "MongoDB": {
      "ConnectionString": "",
      "DatabaseName": "",
      "CollectionNames": {
        "Mission": "",
        "Agent": ""
      }
    }
  }
}
